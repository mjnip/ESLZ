{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "input": {
      "value": {
        "Identity": {
          "type": "SystemAssigned"
        },
        "Location": "canadacentral",
        "Name": "Deploy-FirewallPolicy",
        "Properties": {
          "Description": null,
          "DisplayName": "Deploy-FirewallPolicy",
          "NotScopes": null,
          "Parameters": {
            "fwpolicy": {
              "value": {
                "firewallPolicyName": "eslz-fw-policy-global",
                "location": "canadacentral",
                "ruleGroups": {
                  "name": "DefaultApplicationRuleCollectionGroup",
                  "properties": {
                    "priority": 300,
                    "rules": [
                      {
                        "name": "rule-collection-01",
                        "priority": 100,
                        "ruleType": "FirewallPolicyFilterRule",
                        "action": {
                          "type": "Allow"
                        },
                        "ruleConditions": [
                          {
                            "name": "allow-internet",
                            "protocols": [
                              {
                                "protocolType": "http",
                                "port": 80
                              },
                              {
                                "protocolType": "https",
                                "port": 443
                              }
                            ],
                            "sourceAddresses": [
                              "*"
                            ],
                            "targetFqdns": [
                              "*"
                            ],
                            "fqdnTags": [],
                            "ruleConditionType": "ApplicationRuleCondition"
                          }
                        ]
                      }
                    ]
                  }
                }
              }
            },
            "fwPolicyRegion": {
              "value": "canadacentral"
            },
            "rgName": {
              "value": "contoso-fw-policies"
            }
          },
          "PolicyDefinitionId": "/providers/Microsoft.Management/managementGroups/ESLZ/providers/Microsoft.Authorization/policyDefinitions/Deploy-FirewallPolicy",
          "Scope": "/providers/Microsoft.Management/managementGroups/ESLZ-management"
        },
        "ResourceType": "Microsoft.Authorization/policyAssignments"
      }
    }
  }
}
